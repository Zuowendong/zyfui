{
  "name": "@zyfjs/ui",
  "version": "0.1.1",
  "description": "zyf business component library",
  "keywords": [
    "vue",
    "@zyf",
    "element",
    "ui"
  ],
  "license": "MIT",
  "author": "zuowendong",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "lib",
    "es",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "es/**/style/**.less",
    "lib/**/style/**.less",
    "dist/**.css"
  ],
  "scripts": {
    "build:test": "esno scripts/compile.ts",
    "build:comp": "vite build",
    "bundleLess": "esno scripts/bundleLess.ts",
    "bundleStyle": "vite build --config vite.config.style.ts",
    "bundle": "vite build --config vite.config.bundle.ts",
    "build": "run-s removeBundle build:comp bundleLess bundleStyle bundle"
  },
  "dependencies": {
    "@zyfjs/utils": "workspace:*",
    "vue": "^3.2.40"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/jsdom": "^20.0.0",
    "@types/less": "^3.0.3",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.11.56",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/tsconfig": "^0.1.3",
    "chalk": "^5.1.2",
    "commander": "^9.4.1",
    "cpy": "^9.0.1",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "less": "^4.1.3",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "ora": "^6.1.2",
    "typescript": "^4.8.4",
    "vite": "^3.1.6",
    "vite-plugin-dts": "^1.6.5",
    "vitest": "^0.24.0"
  }
}